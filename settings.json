{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/html": true,
        "**/log": true,
        "**/tmp": true
    },
    "[python]": {
        "editor.defaultFormatter": "KevinRose.vsc-python-indent"
    },
    "git.ignoreLimitWarning": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.showSlider": "always",
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorWidth": 3,
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.fontWeight": "500",
    "editor.fontSize": 16,
    "editor.lineHeight": 24,
    "editor.letterSpacing": 0.5,
    "editor.scrollBeyondLastLine": false,
    "editor.wordWrap": "on",
    "editor.codeLens": true,
    "editor.fontLigatures": true,
    "editor.occurrencesHighlight": "multipleEditors",
    "editor.stickyScroll.enabled": true,
    "editor.linkedEditing": true,
    "workbench.colorTheme": "Live in the Dark",
    "workbench.iconTheme": "catppuccin-perfect-ayu",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.list.horizontalScrolling": true,
    "terminal.integrated.fontFamily": "Courier",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.smoothScrolling": true,
    "window.zoomLevel": 0,
    "window.titleBarStyle": "custom",
    "window.restoreWindows": "all",
    "window.menuBarVisibility": "toggle",
    "telemetry.telemetryLevel": "off",
    "github.copilot.enable": {
        "*": true
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.inlineSuggest.enable": true,
    "github.copilot.features.agent": true,
    "workbench.colorCustomizations": {
        "editorInlayHint.background": "#00000000",
        "editorInlayHint.foreground": "#FFD700",
        "terminal.background": "#490202",
        "terminal.foreground": "#EAEAEA",
        "activityBar.foreground": "#FFD700",
        "activityBar.inactiveForeground": "#CCCCCC",
        "activityBarBadge.background": "#FFD700",
        "activityBarBadge.foreground": "#000000",
        "editor.background": "#1b010e",
        "editor.foreground": "#F0F0F0",
        "editor.selectionBackground": "#55555580",
        "editor.selectionHighlightBackground": "#88888850",
        "editor.lineHighlightBackground": "#ffffff15",
        "editor.lineHighlightBorder": "#ffffff20",
        "editorCursor.foreground": "#FFD700",
        "editorWhitespace.foreground": "#444444",
        "editorIndentGuide.background": "#333333",
        "editorIndentGuide.activeBackground": "#FFD70060",
        "editorBracketHighlight.foreground1": "#FF8C00",
        "editorBracketHighlight.foreground2": "#32CD32",
        "editorBracketHighlight.foreground3": "#7EC8E3",
        "editorBracketHighlight.foreground4": "#FFD700",
        "editorBracketHighlight.foreground5": "#BA55D3",
        "editorBracketHighlight.foreground6": "#20B2AA",
        "editorBracketHighlight.unexpectedBracket.foreground": "#FF4500",
        "editorBracketMatch.background": "#ffffff20",
        "editorBracketMatch.border": "#FFD700",
        "editorGutter.modifiedBackground": "#32CD32",
        "editorGutter.addedBackground": "#1E90FF",
        "editorGutter.deletedBackground": "#FF4500",
        "editorError.foreground": "#FF3333",
        "editorWarning.foreground": "#FFCC00",
        "editorInfo.foreground": "#00AAFF",
        "diffEditor.insertedTextBackground": "#00330050",
        "diffEditor.removedTextBackground": "#66000050",
        "statusBar.background": "#333445",
        "statusBar.foreground": "#FFFFFF",
        "statusBar.noFolderBackground": "#22232D",
        "statusBar.noFolderForeground": "#CCCCCC",
        "statusBar.debuggingBackground": "#550000",
        "statusBar.debuggingForeground": "#FF6347",
        "statusBarItem.remoteBackground": "#FFD700",
        "statusBarItem.remoteForeground": "#000000",
        "sideBar.background": "#2B2B36",
        "sideBar.foreground": "#EAEAEA",
        "sideBarSectionHeader.background": "#3B3B48",
        "sideBarSectionHeader.foreground": "#FFFFFF",
        "sideBarTitle.foreground": "#EAEAEA",
        "editor.overlayBackground": "rgba(255, 255, 255, 0.03)",
        "breadcrumb.foreground": "#CCCCCC",
        "breadcrumb.focusForeground": "#FFFFFF",
        "breadcrumb.activeSelectionForeground": "#FFD700",
        "list.activeSelectionBackground": "#3B3B48",
        "list.activeSelectionForeground": "#FFFFFF",
        "list.hoverBackground": "#333342",
        "list.highlightForeground": "#FFD700"
    },
    "todohighlight.keywords": [
        {
            "text": "TODO",
            "color": "#FFD700",
            "backgroundColor": "#1e1e2e"
        },
        {
            "text": "FIXME",
            "color": "#FF6347",
            "backgroundColor": "#1e1e2e"
        },
        {
            "text": "DEBUG",
            "color": "#00CED1",
            "backgroundColor": "#1e1e2e"
        },
        {
            "text": "HACK",
            "color": "#FFA500",
            "backgroundColor": "#1e1e2e"
        },
        {
            "text": "NOTE",
            "color": "#32CD32",
            "backgroundColor": "#1e1e2e"
        },
        {
            "text": "IMPORTANT",
            "color": "#FF69B4",
            "backgroundColor": "#1e1e2e"
        }
    ],
    "breadcrumbs.enabled": true,
    "editor.rulers": [
        80,
        120
    ],
    "editor.semanticHighlighting.enabled": true,
    "workbench.activityBar.visible": true,
    "workbench.sideBar.location": "left",
    "editor.wordHighlight": true,
    "editor.highlightActiveIndentGuide": true
}